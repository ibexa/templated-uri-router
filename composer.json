{
    "name": "ibexa/templated-uri-router",
    "description": "Symfony2 RFC-6570 compatible router and URL Generator",
    "keywords": [
        "Symfony2",
        "URI",
        "URL",
        "RFC 6570",
        "JSON",
        "XML",
        "Hateoas",
        "Router"
    ],
    "license": "MIT",
    "homepage": "http://www.hautelooktech.com/",
    "authors": [
        {
            "name": "Baldur Rensch",
            "email": "brensch@gmail.com"
        }
    ],
    "replace": {
        "hautelook/templated-uri-router": "self.version"
    },
    "require": {
        "php": ">=5.3.0",
        "symfony/routing": "~2.5|~3.0|^4.0|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35|^5.7|^6.5"
    },
    "autoload": {
        "psr-4": {
            "Hautelook\\TemplatedUriRouter\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "4.0.x-dev"
        }
    }
}
